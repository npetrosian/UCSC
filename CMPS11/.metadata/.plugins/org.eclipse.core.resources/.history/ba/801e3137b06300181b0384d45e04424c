
public class question5 {
	
	public static void probes(int key, int tableSize) {
		int probe;
		for(int i = 0; i < tableSize; i++) {
			probe = (key % tableSize + i*(1 + (key % (tableSize-1)))) % tableSize;
		}
	}

	public static void main(String[] args) {
		System.out.println(probes(34,11));
	}

}
